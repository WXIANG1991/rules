{
 "log": {
        "disabled": false,
        "level": "warn",
        "timestamp": true
    },
    "dns": {
        "servers": [
          { "tag": "dns_block", "address": "rcode://refused" },
          { "tag": "dns_alidns", "address": "h3://223.5.5.5/dns-query", "detour": "DIRECT" },
          { "tag": "dns_dnspod", "address": "https://1.12.12.12/dns-query", "detour": "DIRECT" },
          { "tag": "dns_proxy", "address": "h3://1.0.0.1/dns-query", "detour": "ðŸš€ èŠ‚ç‚¹é€‰æ‹©" },
          { "tag": "dns_proxy2", "address": "h3://9.9.9.9/dns-query", "detour": "ðŸš€ èŠ‚ç‚¹é€‰æ‹©" }
        ],
        "rules": [
          { "outbound": "any", "server": "dns_dnspod" },
          { "clash_mode": "Direct", "server": "dns_dnspod" },
          { "clash_mode": "Global", "server": "dns_proxy" },
          { "rule_set": [ "microsoft-cn", "apple-cn", "google-cn", "cn", "private","direct-self" ], "query_type": [ "A", "AAAA" ], "server": "dns_dnspod" }
        ],
        "final": "dns_proxy",
        "strategy": "prefer_ipv4",
        "independent_cache": true,
        "reverse_mapping": true,
        "fakeip": { "enabled": false, "inet4_range": "198.18.0.0/15", "inet6_range": "fc00::/18" }
    },
    "ntp": {
        "enabled": true,
        "server": "time.apple.com",
        "server_port": 123,
        "interval": "30m",
        "detour": "DIRECT"
    },
    "inbounds": [
        {
            "type": "mixed",
            "tag": "mixed-in",
            "listen": "0.0.0.0",
            "listen_port": 2080
        },
        {
            "type": "tun",
            "tag": "tun-in",
            "inet4_address": "172.19.0.1/30",
            "auto_route": true,
            "strict_route": true,
            "stack": "mixed",
            "sniff": true
        }
    ],
    "experimental": {
        "cache_file": {
            "enabled": true,
            "store_fakeip": false
        },
        "clash_api": {
            "external_controller": "127.0.0.1:9090",
            "external_ui": "dashboard"
        }
    },
    "route": {
        "auto_detect_interface": true,
        "geosite": {
            "path": "./geosite.db",
            "download_url": "https://cdn.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@release/geosite.db"
        },
        "geoip": {
            "path": "./geoip.db",
            "download_url": "https://cdn.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@release/geoip.db"
        }
    }
}
